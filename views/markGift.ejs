<body>
    <div class="row">
        <div class="col-md-10 col-md-offset-1 col-sm-12 col-xs-12">
            <h4>Marcar presente</h4>
            <form action="" class="form-mark" method="post">
                <div class="row">
                    <div class="col-xs-12 form-group">
                        <input type="text" class="form-control" id="input-presente" disabled value="<%=gift.name%>"/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 form-group">
                        <input type="text" class="form-control" name="name" placeholder="Nome Completo"/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 col-sm-6 form-group">
                        <input type="text" class="form-control" name="email" placeholder="E-mail"/>
                    </div>
                    <div class="col-xs-12 col-sm-6 form-group">
                        <input type="text" class="form-control" name="phone" placeholder="telefone"/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 col-md-3 col-md-offset-9 text-right">
                        <button type="submit" class="btn btn-block btn-lg btn-success">Salvar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <!-- footer close -->
