        <div class="container-fluid">
            <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-12 col-xs-12 table-responsive text-center">
                    <small>Clique no nome do presente para visualizar em uma loja virtual</small>
                    <table class="table text-left">
                        <thead>
                            <tr>
                                <th>Presente</th>
                                <th>Status</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <%gifts.forEach(gift => {%>
                                <tr>
                                    <td><a target="_blank" href="<%=gift.link%>"><%=gift.name%></a></td>
                                    <td>
                                        <%=gift.status%>
                                    </td>
                                    <td>
                                        <%if(gift.status === 'pendente'){%>
                                            <a href="#" onclick="return openFormMark('<%=gift._id%>')">Marcar</a>
                                        <%}%>
                                    </td>
                                </tr>
                            <%})%>
                        </tbody>
                    </table>
                </div>
            </div>

            <script>
                function openFormMark(id){
                    alert(id)
                    return false;
                }
            </script>
    
            <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-12 col-xs-12">
                    <h4>Marcar presente</h4>
                    <form action="" class="form-mark">
                        <div class="row">
                            <div class="col-xs-12 form-group">
                                <input type="text" class="form-control" id="input-presente" placeholder="Presente"/>
                            </div>
                        </div>
                        <div class="row">
